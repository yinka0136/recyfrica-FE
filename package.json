{
  "name": "recyfrica",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "build": "npm run build:ssr",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run recyfrica:serve-ssr",
    "serve:ssr": "node dist/recyfrica/server/main.js --proxy-config proxy.config.json",
    "build:ssr": "ng build --configuration production && ng run recyfrica:production",
    "prerender": "ng run recyfrica:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
